{
  "source": {
    "include": [
      "app.js",
      "test",
      "server",
      "routes",
      "model",
      "controllers"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./documentation",
    "readme": "./README.md"
  }
}